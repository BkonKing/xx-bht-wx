page {
  background: #fafafa;
}

.invoice-box {
  display: flex;
  min-height: 180rpx;
  padding: 30rpx;
  position: relative;
  margin: 30rpx 30rpx 0;
  background: #FFFFFF;
  border-radius: 4rpx;
}

.invoice-select {
  display: flex;
  flex: 1;
  justify-content: flex-end;
  align-items: center;
  padding-right: 42rpx;
  color: #AAAAAA;
}

.order-number {
  display: flex;
  margin-bottom: 12rpx;
}

.invoice-select text {
  color: #EF4141;
}

.invoice-session {
  position: absolute;
  top: 80rpx;
  right: 30rpx;
  display: flex;
  align-items: center;
}

.invoice-session image {
  width: 12rpx;
  height: 22rpx;
}

.invoice-image {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 120rpx;
  height: 120rpx;
  margin-right: 30rpx;
  background: #EEEEEE;
  border-radius: 2rpx;
}

.invoice-image .order-image {
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
}

.invoice-image .no-order-image {
  width: 36rpx;
  height: 44rpx;
}

.invoice-content {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  font-size: 28rpx;
  color: #212124;
}

.invoice-content image {
  width: 32rpx;
  height: 32rpx;
  margin-left: 20rpx;
}

.flex-align-center {
  display: flex;
  align-items: center;
}

.invoice-label {
  margin-right: 20rpx;
}

.invoice-strong {
  font-weight: 800;
}

.shrink-box {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 58rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  background-color: #fff;
  z-index: 2;
}

.shrink-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 78rpx;
  height: 36rpx;
  background: #F7F7F7;
  border-radius: 18rpx;
}

.shrink-btn image {
  width: 18rpx;
  height: 10rpx;
}

.fold-list {
  position: relative;
  height: 406rpx;
  overflow: hidden;
  padding-bottom: 59rpx;
}

.bottom-form-list {
  padding-bottom: 58rpx;
}

/* 展开 */
.unfold-btn {
  position: absolute;
}

/* 折叠 */
.fold-btn {}

.form-list {
  position: relative;
  margin: 30rpx 30rpx 0;
  border-radius: 4px;
  background-color: #fff;
}

.form-item {
  display: flex;
  align-items: flex-start;
  min-height: 86rpx;
  padding: 0 30rpx;
  position: relative;
}

.form-item::after {
  left: 30rpx;
  right: 30rpx;
}

.item-name {
  width: 140rpx;
  padding-top: 28rpx;
  color: #212124;
  line-height: 1;
}

.itemt-text {
  /* width: 532rpx; */
  display: flex;
  justify-content: flex-end;
  flex-grow: 1;
  margin-left: 10rpx;
  min-height: 86rpx;
  padding: 20rpx 0;
}

.line {
  border-bottom: 2rpx solid #EEEEEE;
}


.text-right {
  text-align: right;
}

.radio-group {
  display: flex;
  align-items: center;
}

.item-radio {
  display: flex;
  align-items: center;
}

.item-radio+.item-radio {
  margin-left: 60rpx;
}

.item-radio radio .wx-radio-input {
  width: 32rpx;
  height: 32rpx;
  margin-top: -6rpx;
  margin-right: 16rpx;
}

.itemt-icon {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 52rpx;
  padding: 6rpx 0 6rpx 20rpx;
}

.itemt-icon image {
  width: 32rpx;
  height: 32rpx;
}

.itemt-text input {
  width: 100%;
  height: 100%;
  font-size: 28rpx;
  color: #212124;
  text-align: right;
}

.placeholder {
  color: #bbbbbb;
}

.required-after::after {
  content: '*';
  color: #EF4141;
}

.label-session {
  justify-content: space-between;
}

.label-list-session {
  flex-grow: 1;
  width: 0;
  display: flex;
  justify-content: space-between;
}

.label-list-session.label-list-right {
  justify-content: flex-end;
}

.label-list {
  display: flex;
  padding-top: 28rpx;
  height: 88rpx;
}

.label-item {
  margin-right: 20rpx;
  border: 2rpx solid #b8babf;
  line-height: 58rpx;
  height: 58rpx;
  color: #919499;
  padding: 0 24rpx;
  border-radius: 30rpx;
}

.label-item.cur {
  background-color: #b8babf;
  color: #ffffff;
}

.label-item:last-child {
  margin-right: 0;
}

.custom-session {
  float: right;
  display: flex;
  align-items: center;
  color: #919499;
  line-height: 1;
}

.custom-session image {
  width: 12rpx;
  height: 22rpx;
  margin-left: 20rpx;
}

.form-item.default-item {
  align-items: center;
  justify-content: space-between;
}

.default-item .item-name {
  width: 250rpx;
}

.address-label {
  display: inline-block;
  min-width: 60rpx;
  height: 36rpx;
  padding: 0 10rpx;
  margin-left: 20rpx;
  background: #CB9348;
  border-radius: 2px;
  font-size: 22rpx;
  font-weight: 500;
  text-align: center;
  line-height: 36rpx;
  color: #FFFFFF;
}

.address-user {
  margin-left: 20rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: #212124;
}

.address-info {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  margin-top: 10rpx;
  margin-bottom: 15rpx;
}

.address-text {
  margin-bottom: 2rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: #AAAAAA;
  line-height: 44rpx;
}

.btn-kong {
  height: 148rpx;
}

.btn-box {
  display: flex;
  justify-content: space-between;
  padding: 30rpx;
}

.btn-box view {
  width: 335rpx;
}

.btn-box view.btn-100 {
  width: 100%;
}

.del-btn {
  background-color: #b8babf;
}

.sure-btn {
  border-radius: 44rpx;
}

.sure-btn.disabled-btn {
  background-color: #919499;
}

.swal-title {
  position: relative;
  margin-bottom: 20rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #212124;
  text-align: center;
}

.swal-content {
  padding-top: 14rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: #212124;
}

.swal-session {
  animation: translateMove 0.5s;
  -webkit-animation: translateMove 0.5s;
  background-color: #fff;
  height: 340rpx;
  z-index: 188;
  padding: 40rpx 30rpx 0;
}

.type-session {
  position: fixed;
  width: 560rpx;
  height: auto;
  padding: 0;
  top: 50%;
  left: calc(50% - 280rpx);
  right: 0;
}

.type-list {
  padding: 0 20rpx;
  background: #FFFFFF;
  border-radius: 4rpx;
}

.type-item {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 106rpx;
  color: #212124;
}

.type-item+.type-item {
  border-top: 2rpx solid #EEEEEE;
}

.close-btn {
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  top: -8rpx;
  right: -13rpx;
  z-index: 189;
  padding: 10rpx;
  display: flex;
}

@keyframes translateMove {
  from {
    transform: translateY(100%);
  }

  to {
    transform: translateY(0);
  }
}

@-webkit-keyframes translateMove {
  from {
    transform: translateY(100%);
  }

  to {
    transform: translateY(0);
  }
}

.item-checkbox {
  margin-top: -8rpx;
  margin-right: -12rpx;
}

.item-checkbox .wx-checkbox-input {
  display: block;
  width: 32rpx;
  height: 32rpx;
  text-align: center;
  border-radius: 50%;
}

/*checkbox选中后样式  */
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border-color: #000;
  background: #000;
}

/*checkbox选中后图标样式  */
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  font-size: 32rpx;
}