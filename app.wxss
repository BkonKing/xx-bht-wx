/* empty */
.empty-none {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  position: absolute;
  width: 100%;
  bottom: 0;
  top: 88px;
  padding-top: 196rpx;
  flex-direction: column;
}
.empty-none image {
  /* width: 580rpx;
  height: 440rpx; */
  width: 377rpx;
  height: 260rpx;
}
.empty-none view {
  color: #919499;
  font-size: 34rpx;
  line-height: 82rpx;
  width: 100%;
  text-align: center;
}

.clearfix::after {
  content:""; 
  display: block; 
  clear:both; 
}
.p-nowrap {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.p-nowrapm {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
}
.btn-d {
  height: 88rpx;
  width: 100%;
  line-height: 88rpx;
  text-align: center;
  color: #fff;
  background-color: #212124;
  position: relative;
  font-size: 30rpx;
  font-weight: normal;
}
.btn-d::after {
  border: none;
}
.btn-red {
  color: #fff;
  background-color: #eb3449 !important;
}
.btn-yellow {
  color: #fff;
  background-color: #ffc90f;
}
.btn-border {
  height: 66rpx;
  font-size: 28rpx;
  line-height: 64rpx;
  border-radius: 33rpx;
  text-align: center;
  margin: 0 auto;
  box-sizing: border-box;
  border: 1px solid #ffb61a;
  color: #ffb61a;
  padding: 0 37rpx;
}
.btn-border::after {
  border: none;
}

.bottom-fixed {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 20;
}



.mask-block {
  position: fixed;
  z-index: 9999;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.6);
}
.poster-mask {
  z-index: 9999;
  background-color: #fff;
}

.width-100 {
  width: 100%;
  height: 100%;
}

