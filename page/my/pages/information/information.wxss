page {
  background: #fafafa;
}
.form-list {
  padding: 0 30rpx;
  background-color: #fff;
}
.form-list:nth-child(2) {
  margin-top: 30rpx;
}
.form-item {
  display: flex;
  border-bottom: 1px solid #f1f1f1;
  height: 121rpx;
}
.form-item:last-child {
  border: none;
}
.item-name {
  width: 158rpx;
  line-height: 120rpx;
  color: #60646b;
}
.item-name text {
  color: #f33f3f;
}
.itemt-text {
  /* width: 532rpx; */
  flex-grow: 1;
  padding: 30rpx 0;
  padding-right: 10rpx;
}
.tel-itemt.itemt-text {
  padding: 0 10rpx 0 0;
}
.itemt-icon {
  width: 12rpx;
  justify-content: center;
  display: flex;
  align-items: center;
  margin-right: 6rpx;
}
.itemt-icon image {
  width: 12rpx;
  height: 22rpx;
}
.itemt-text input,.itemt-text button {
  width: 100%;
  height: 100%;
  font-size: 30rpx;
  color: #212124;
  float: left;
}
.placeholder-text {
  color: #b8babf;
}
.itemt-text button:not([size='mini']) {
  margin: inherit;
  margin-bottom: 0;
  font-size: 30rpx;
  font-weight: normal;
  padding: 0;
  line-height: 120rpx;
  background-color: #fff;
  width: 100%;
  text-align: left;
}

.information-btn {
  position: fixed;
  bottom: 40rpx;
  left: 30rpx;
  right: 30rpx;
  width: 690rpx;
}