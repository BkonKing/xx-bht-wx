page{
  overflow-x: hidden;
  line-height: 1.6;
  font-size: 30rpx;
  font-family: -apple-system-font, "Helvetica Neue", sans-serif;
}
view,navigator {
  box-sizing: border-box;
}
.nomore {
  text-align: center;
  font-size: 14px;
  color: #999;
  height: 103rpx;
  line-height: 102rpx;
  position: relative;
}
.nomore::before,.nomore::after {
  content: '';
  position: absolute;
  top: 51rpx;
  height: 1rpx;
  width: 116rpx;
}
.nomore::before {
  left: 180rpx;
  background: -webkit-linear-gradient(to left, #b3b3b3 , #fafafa);
  background: linear-gradient(to left, #b3b3b3 , #fafafa);
}
.nomore::after {
  right: 180rpx;
  background: -webkit-linear-gradient(to right, #b3b3b3 , #fafafa);
  background: linear-gradient(to right, #b3b3b3 , #fafafa);
}
.normal{
  background: #ddd!important;
  box-shadow: 0 2px 6px 1px rgba(119, 119, 119, 0.4)!important;
}


/* 弹出层 */
.maskbg,.loginBg,.modal-bg {
  background: rgba(0,0,0,0.4);
  width: 100%;
  height: 100%;
  margin: auto;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 99;
}
.loginBg {
  z-index: 45;
}
