.alert-session {
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 985;
}
.alert-block {
  width: 600rpx;
  min-height: 368rpx;
  border-radius: 4rpx;
  background-color: #fff;
  font-size: 30rpx;
  position: relative;
}
.close-btn {
  position: absolute;
  width: 88rpx;
  height: 88rpx;
  top: -88rpx;
  right: 10rpx;
  z-index: 66;
  padding: 20rpx;
  display: flex;
}
.close-btn image {
  width: 100%;
  height: 100%;
}
.alert-header {
  height: 100rpx;
  line-height: 86rpx;
  padding-top: 14rpx;
  text-align: center;
  font-size: 34rpx;
  color: #212124;
  font-weight: bold;
}
.alert-txt {
  line-height: 49rpx;
  letter-spacing: 1.2rpx;
  color: #60646b;
  width: 500rpx;
  min-height: 146rpx;
  margin: 0 auto;
}
.alert-line {
  height: 1px;
  width: 560rpx;
  margin: 0 auto;
  background-color: #f1f1f1;
}
.alert-btn {
  height: 120rpx;
  padding: 32rpx 0 28rpx;
  display: flex;
  align-items: center;
}
.alert-btn view,
.alert-btn navigator {
  height: 100%;
  line-height: 60rpx;
  flex: 1;
  text-align: center;
}
.alert-btn view {
  color: #919499;
  border-right: 1px solid #f1f1f1;
}
.alert-btn navigator,.alert-btn view.color-212124 {
  color: #212124;
}

@keyframes myfain
{
  from {opacity:0;}
  to {opacity:1;}
}

@-webkit-keyframes myfain
{
  from {opacity:0;}
  to {opacity:1;}
}