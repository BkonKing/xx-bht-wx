.nav-border {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 55;
  height: 1px;
  background-color: #efefef;
}

.bht-session {
  width: 690rpx;
  margin: 0 auto;
  border-radius: 4rpx;
  overflow: hidden;
  background-color: #fff;
}

.bottom-line::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 1px;
  background-color: #efefef;
  z-index: 5;
}

@keyframes myfain
{
  from {opacity:0;}
  to {opacity:1;}
}
@-webkit-keyframes myfain
{
  from {opacity:0;}
  to {opacity:1;}
}